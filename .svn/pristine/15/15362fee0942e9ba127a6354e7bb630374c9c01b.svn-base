@charset "utf-8";
/*====================================================
	common styles
====================================================*/
html,body {
	color: #505050;
	line-height: 1em;
	background: #f9f9f9;
	position: relative;
	margin: 0;
	font-family: "Microsoft YaHei", "Xin Gothic", "Hiragino Sans GB", "Droid Sans Fallback", "Georgia", sans-serif;
}
button {
	width: 80px;
	height: 28px;
	margin-left: 5px;
	margin-right: 5px;
	color: #008080;
	font-weight: bold;
	display: inline-block;
	overflow: hidden;
}
button:disabled {
	color:#808080;
}
button:hover {
	cursor:hand;
}
input {
	height: 25px;
}
select {
	height: 25px;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: 400;
  color: #303030;
}
h1 {
  font-size: 3em;
}
h2 {
  font-size: 2.5em;
}
h3 {
  font-size: 2em;
}
h4 {
  margin-top: 25px;
  font-size: 1.5em;
}
h5 {
  font-size: 1em;
}
h6 {
  font-size: 0.75em;
}
a {
  color: #505050;
  outline: none;
}
a:hover {
  color: #009933;
}
a:active,
a:focus,
a img {
  outline: none;
}

