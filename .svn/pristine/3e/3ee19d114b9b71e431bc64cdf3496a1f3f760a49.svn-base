<script>
    $(document).ready(function(){     
     
     if($(".regular").html().trim() == '') {
    	 $(".regular").append("<div><img src='/makita/images/pictures/Default.jpg'/></div>");
     };
     
     $("img").on("error",function(){
    	 this.src='/makita/images/pictures/NotFound.jpg';
    	 $(this).unbind();
     });

     $(".regular").slick({
        dots: true,
        infinite: true,
        autoplay:true,
        slidesToShow: 1,
        slidesToScroll: 1
     });
     
     });
</script>
