Button.appleButton{
  height:60px;
  width:260px;
  color:#000000;
  background-color:#ededed;
  font-size:16px;
  font-weight:normal;
  font-family:Arial;
  background:-webkit-gradient(linear, left top, left bottom, color-start(0.05, #ffffff), color-stop(1, #00fffc));
  background:-moz-linear-gradient(top, #ffffff 5%, #00fffc 100%);
  background:-o-linear-gradient(top, #ffffff 5%, #00fffc 100%);
  background:-ms-linear-gradient(top, #ffffff 5%, #00fffc 100%);
  background:linear-gradient(to bottom, #ffffff 5%, #00fffc 100%);
  background:-webkit-linear-gradient(top, #ffffff 5%, #00fffc 100%);
  filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#00fffc',GradientType=0);
  border:1px solid #006c14;
  -webkit-border-top-left-radius:8px;
  -moz-border-radius-topleft:8px;
  border-top-left-radius:8px;
  -webkit-border-top-right-radius:8px;
  -moz-border-radius-topright:8px;
  border-top-right-radius:8px;
  -webkit-border-bottom-left-radius:8px;
  -moz-border-radius-bottomleft:8px;
  border-bottom-left-radius:8px;
  -webkit-border-bottom-right-radius:8px;
  -moz-border-radius-bottomright:8px;
  border-bottom-right-radius:8px;
  -moz-box-shadow:3px 4px 0px -20px #1564ad;
  -webkit-box-shadow:3px 4px 0px -20px #1564ad;
  box-shadow:3px 4px 0px -20px #1564ad;
  text-align:center;
  display:inline-block;
  text-decoration:none;
}
.appleButton:hover{
  background-color:#f5f5f5;
  background:-webkit-gradient(linear, left top, left bottom, color-start(0.05, #00fffc), color-stop(1, #ffffff));
  background:-moz-linear-gradient(top, #00fffc 5%, #ffffff 100%);
  background:-o-linear-gradient(top, #00fffc 5%, #ffffff 100%);
  background:-ms-linear-gradient(top, #00fffc 5%, #ffffff 100%);
  background:linear-gradient(to bottom, #00fffc 5%, #ffffff 100%);
  background:-webkit-linear-gradient(top, #00fffc 5%, #ffffff 100%);
  filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#00fffc', endColorstr='#ffffff',GradientType=0);
}

